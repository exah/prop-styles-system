{
  "env": {
    "modules": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  },
  "presets": [
    "flow",
    [ "env", { "modules": false } ]
  ],
  "plugins": [
    [ "transform-runtime", { "helpers": true, "polyfill": false } ],
    [ "transform-object-rest-spread", { "useBuiltIns": true } ]
  ]
}
